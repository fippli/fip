// Test every?, some?, none?, defined?
numbers: [2, 4, 6]
is-two?: (n) { n = 2 }
all-two: every?(is-two?, numbers)

some-numbers: [1, 2, 3]
has-two: some?((n) { n = 2 }, some-numbers)

odd-numbers: [1, 3, 5]
no-zero: none?((n) { n = 0 }, odd-numbers)

empty-list: []
all-empty: every?((n) { n = 1 }, empty-list)
some-empty: some?((n) { n = 1 }, empty-list)
none-empty: none?((n) { n = 1 }, empty-list)

test-null: null
test-value: 42
is-null-defined: defined?(test-null)
is-value-defined: defined?(test-value)

log!(all-two)
log!(has-two)
log!(no-zero)
log!(all-empty)
log!(some-empty)
log!(none-empty)
log!(is-null-defined)
log!(is-value-defined)

