log!("== Functions and Currying ==")

add3: (x, y, z) { x + y + z }
log!(add3(10, 20, 30))

add1: add3(1)
add1-and-2: add1(2)
log!(add1-and-2(3))

add-ten: add3(10)
add-ten-and-five: add-ten(5)
log!(add-ten-and-five(1))

double: (n) { n * 2 }
increment: (n) { n + 1 }
pipeline: (value) {
  value
  double
  increment
}

log!(pipeline(5))
